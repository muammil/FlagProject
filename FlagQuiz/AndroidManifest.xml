<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.quiz.flag"
  android:versionCode="1"
  android:versionName="1.0" >
  <uses-sdk
    android:minSdkVersion="14"
    android:targetSdkVersion="17" />
  <application
    android:allowBackup="true"
    android:icon="@drawable/logo"
    android:label="@string/app_name"
    android:theme="@style/AppTheme" >
  <activity
    android:name="com.quiz.flag.FlagMainActivity"
    android:label="@string/app_name" >
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
  </activity>
  <activity android:name="com.quiz.flag.FlagListActivity"/>
  <activity android:name="com.quiz.flag.NameToFlagActivity"
    android:screenOrientation="portrait"/>
  <activity android:name="com.quiz.flag.FlagToNameActivity"
    android:screenOrientation="portrait"/>
  <activity android:name="com.quiz.flag.CountDownActivity"
    android:screenOrientation="portrait"/>
  </application>
</manifest>
